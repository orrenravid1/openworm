    Soma { pt3dclear() }
    Soma { pt3dadd( -0.65000004, -226.04999, 2.35, 3.254228 ) }
    Soma { pt3dadd( -0.65000004, -222.795762, 2.35, 3.254228 ) }
    // Section: Axon, id: Axon, parent: Soma (entry count 2/100)
